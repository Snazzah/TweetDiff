{
  "name": "Tweet Diff",
  "description": "Diff checker on tweet edit history",
  "manifest_version": 3,
  "homepage_url": "https://github.com/Snazzah/TweetDiff",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://x.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}